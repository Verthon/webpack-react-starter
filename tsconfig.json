{
	"compilerOptions": {
		"outDir": "./dist/",
		"sourceMap": true,
		"strict": true,
		"module": "CommonJS",
		"target": "es5",
		"jsx": "react-jsx",
		"moduleResolution": "node",
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true,
		"lib": ["es6", "dom"],
	},
	"include": ["./src/**/*"],
  "exclude": ["node_modules"]
}
